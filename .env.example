ASPNETCORE_ENVIRONMENT=Production

# ?? Docker Container ?????????????????????????????????????????
CONTAINER_NAME=onemanvanfsm-web
DATA_VOLUME=/media/AppDatabases/OneManVanFSMData

# ?? Database Names ???????????????????????????????????????????
APP_DB_NAME=OneManVanFSM
IDENTITY_DB_NAME=OneManVanIdentity

# ?? Admin Credentials ????????????????????????????????????????
ADMIN_EMAIL=
ADMIN_PASSWORD=

# ?? Connection ???????????????????????????????????????????????
CONNECTION_TIMEOUT=30

# ?? Mobile Sync ??????????????????????????????????????????????
SYNC_USER=sync_user
SYNC_PASSWORD=
